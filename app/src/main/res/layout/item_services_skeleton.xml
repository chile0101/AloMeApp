        <android.support.v7.widget.CardView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/padding_8" android:layout_marginTop="@dimen/padding_2" android:layout_marginRight="@dimen/padding_2" android:layout_marginBottom="@dimen/padding_2" android:layout_weight="1" app:cardCornerRadius="@dimen/padding_8">
